{
  "name": "spotify-clone",
  "version": "1.0.0",
  "description": "This app uses spotify's authorization code with Oauth to recreate the basic functionality of spotify",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammyk118/spotify-clone.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sammyk118/spotify-clone/issues"
  },
  "homepage": "https://github.com/sammyk118/spotify-clone#readme",
  "dependencies": {
    "express": "^4.17.1",
    "unirest": "^0.6.0"
  }
}
